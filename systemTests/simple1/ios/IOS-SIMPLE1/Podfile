# Uncomment this line to define a global platform for your project
# platform :ios, '6.0'

# J2ObjC Gradle Plugin - DO NOT MODIFY from here to the first target
def j2objc_simple1
    pod 'j2objc-simple1-debug', :configuration => ['Debug'], :path => '../../build'
    pod 'j2objc-simple1-release', :configuration => ['Release'], :path => '../../build'
end

target 'IOS-SIMPLE1' do
    platform :ios, '6.0'
    j2objc_simple1
end

target 'IOS-SIMPLE1Tests' do
    platform :ios, '6.0'
    j2objc_simple1
end
